{
  "vscode": {
    "select-inside-tag": "ctrl + cmd + shift + arrow left/right",
    "select-ocurrence": "cmd + d",
    "select-all-ocurrences": "alt + cmd + shift + arrow down/up",
    "fold-collapse-blocks": "alt + cmd + [ / ]",
    "move-cursor-beginning-end": "cmd + arrow left/right",
    "select-entire-line": "cmd + shift + arrow left/right",
    "fold-everything": "cmd + k + cmd + 0",
    "unfold-everything": "cmd + k + cmd + j",
    "clipboard-history": "cmd + shift + v",
    "command-palette": "cmd + shift + p",
    "quick-navigation": "cmd + p",
    "search-symbol": "cmd + t",
    "go-to-definition": "F12",
    "intellisense": "ctrl + space",
    "new-window": "cmd + shift + n",
    "integrated-terminal": "cmd + `",
    "side-bar": "cmd + b",
    "search": "cmd + f",
    "save": "cmd + s",
    "zen-mode": "cmd + k + z",
    "copy-line": "shift + opt + up/down",
    "create-and-open-file": "cmd + click",
    "move-entire-line": "opt + up/down",
    "move-and-copy-entire-line": "opt + shift + up/down",
    "select-current-line": "cmd + i",
    "multi-cursor-selection": "opt + cmd + up/down"
  },
  "chrome": {
    "undo-closed-tab": "cmd + shift + tab"
  },
  "hyper": {
    "dropdown-hyper": "opt + space",
    "open-config-file": "cmd + ,"
  },
  "macos": {
    "open": "open [directory]",
    "toggle-windows-same-app": "cmd + `",
    "screenshot-single-window": "cmd + shift + 4 + space",
    "key-repeat": "fast",
    "delay-until-repeat": "short",
    "do-not-remove-from-startup": ["watchman", "haxm", "virtualbox"],
    "toggle-hidden-files": "cmd + shift + ."
  },
  "maintenance": {
    "screenshots":
      "defaults write com.apple.screencapture location ~/folder-for-screenshots",
    "disable-screenshot-shadow":
      "defaults write com.apple.screencapture disable-shadow -bool true",
    "dock": "defaults write com.apple.dock autohide-time-modifier -int 0",
    "git-email": "git config --global user.email lndgalante@gmail.com",
    "git-username": "git config --global user.name lndgalante",
    "zsh": "~/.zshrc",
    "deactivate-gatekeeper": "sudo spctl --master-disable",
    "brew": "brew update && brew upgrade && brew cleanup"
  },
  "network": {
    "ip-private": "DHCP",
    "dns-primary-ipv4": "8.8.8.8",
    "dns-secondary-ipv4": "8.8.4.4",
    "dns-primary-ipv6": "2001:4860:4860::8888",
    "dns-secondary-ipv6": "2001:4860:4860::8844"
  },
  "preferences": {
    "monitor": "Color LCD"
  }
}
