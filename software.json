{
    "operative-system": {
        "name": "macOS Sierra",
        "version": "10.12.5"
    },
    "package-manager": {
        "name": "Homebrew & Cask",
        "version": "1.2.0"
    },
    "terminal": {
        "shell": "oh-my-zsh",
        "prompt": "pure-prompt",
        "theme": "snazzy",
        "plugins": [
            "react-native",
            "git"
        ]
    },
    "primary-web-browser": {
        "name": "Google Chrome",
        "ver": "58.0.3029.110",
        "extensions": [
            "React Developer Tools",
            "Redux Developer Tools",
            "uBlock Origin",
            "JSON Viewer",
            "Lighthouse",
            "Wappalyzer",
            "PageRuler",
            "Octotree",
            "Momentum",
            "Postman"
        ]
    },
    "code-editor": {
        "name": "Visual Studio Code",
        "ver": "1.12.2",
        "extensions": [
            "ReactJS Code Snippets",
            "React Native Tools",
            "Path Intellisense",
            "HTML CSS Support",
            "npm Intellisense",
            "EditorConfig",
            "Better Merge",
            "Prettier",
            "Beautify",
            "ESLint"
        ],
        "color-theme": "Material Theme (vsc-material-theme)",
        "icon-theme": "Material Theme Icons",
        "font": "Operator Mono"
    },
    "office-suite": {
        "name": "Microsoft Office",
        "ver": "15.33"
    },
    "multimedia-player": {
        "name": "VLC",
        "ver": "2.2.4"
    },
    "torrent-manager": {
        "name": "WebTorrent",
        "ver": "0.18.0"
    },
    "chat-app": {
        "name": "Slack",
        "ver": "2.6.1"
    },
    "cleaner": {
        "name": "Ccleaner",
        "ver": "1.14.451"
    },
    "ntfs": {
        "name": "Mounty",
        "ver": "1.3"
    },
    "decompress": {
        "name": "The Unrarchiver",
        "ver": "3.11.1"
    },
    "gif-recorder": {
        "name": "RecordIt",
        "ver": "1.0.134"
    }
}